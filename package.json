{
  "name": "@eyecuelab/recoil-devtools",
  "version": "0.1.7",
  "license": "MIT",
  "authors": [
    "joshwrn",
    "jeremybanka"
  ],
  "main": "dist/index.js",
  "module": "dist/recoil-devtools.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eyecuelab/recoil-devtools.git"
  },
  "keywords": [
    "recoil",
    "devtools",
    "react",
    "state",
    "atom",
    "selector"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests",
    "release": "yarn build && changeset publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 81,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/recoil-devtools.cjs.production.min.js",
      "limit": "1000 KB"
    },
    {
      "path": "dist/recoil-devtools.esm.js",
      "limit": "1000 KB"
    }
  ],
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17",
    "recoil": ">=0.7.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@babel/core": "^7.20.12",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.20.7",
    "@eyecuelab/eslint-config-react": "^1.1.0",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@tsconfig/create-react-app": "^1.0.2",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@types/react-portal": "^4.0.4",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "dts-cli": "^1.6.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.5",
    "size-limit": "^8.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "framer-motion": "^7.3.2",
    "react-icons": "^4.4.0",
    "react-is": "^18.2.0",
    "react-portal": "^4.2.2",
    "styled-components": "^5.3.5"
  }
}
